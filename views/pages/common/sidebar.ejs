<aside id="sidebar" class="well well-lg">
    <h2>
        Catégories
    </h2>
    <ul class="list-group">
        <% for (var i = 0; i < contient.length; i++) { %>
        <li id="contenantCatégorie" class="list-group-item d-flex justify-content-between align-items-center">
            <% var link='href=/Categories/'+contient[i][0].NumCatégorie+'/Page/1' %>
            <a id="texteCatégorie" <%= link %>> <%= contient[i][0].LibelléCatégorie %></a>
            <span id="nombreCatégorie" class="badge badge-primary badge-pill"> <%= contient[i][0].ite %></span>
        </li>
        <% } %>
    </ul>
</aside>